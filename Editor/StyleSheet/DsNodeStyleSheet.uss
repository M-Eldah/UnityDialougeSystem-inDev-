.SpeachText
{
	max-width: 260px;
	flex-direction:column;
	justify-content: flex-start;
}
.unity-text-field__input
{
	/*Allows the text box to expand to fit the text once maximum length have been reached*/
	white-space:Normal;
}
.unity-text-field
{
	/*Allows the text box to expand to fit the text once maximum length have been reached*/
	white-space:Normal;
}
