/*Containes*/
.ActionNode
{
	background-color:#c0392b;
}
.ActionNode>*>*>*>*>*>.unity-foldout{
	padding: 8px;
	background-color:#e74c3c;
}
.ActionNode>*>*>*>*>*>.unity-foldout>#unity-content{
	padding: 8px;
	background-color:#c0392b;
	border-radius:4px;
}
.ActionNode>#contents>#top>#output>Port{
	padding: 8px;
	background-color:#c0392b;
	border-radius:4px;
}
